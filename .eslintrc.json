{
  "env": {
    "browser": true,
    "es2021": true,
    "jest": true
  },
  "extends": [
    "react-app",
    "react-app/jest",
    "airbnb",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "react/no-unescaped-entities": ["off"],
    "react/prop-types": [
      "off",
      {
        "ignore": [],
        "customValidators": null,
        "skipUndeclared": null
      }
    ]
  }
}
